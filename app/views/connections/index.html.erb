<h1>Connections#index</h1>
<div class="container text-center">
  <% @matches.each_with_index do |match, index| %>
    <div class="card">
      <h3><%= match[0].first_name %> <%= match[0].last_name %></h3>
      <p><%= match[1] %></p>
      <%= link_to "Message", connection_messages_path(@connections[index]), class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
